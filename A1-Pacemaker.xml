	<system>// Declaration goes here.
broadcast chan LRI_Start, VRP_Start, LRI_Stop;
broadcast chan VSense, VP, LRI_Ex, VRP_Ex, sync_test, DT;

// Atrial signals
broadcast chan ASense, AP;
broadcast chan PVARP_Start, AEI_Start, PVARP_Done, PVARP_Stop, AP_Ex, AEI_Done, AEI_Stop;
broadcast chan AVI_Ex, AVI_Done, AVI_Start, AVI_Stop;

// Ventricle Components- note, need to add AVI constraints
Random_Ventricle = RV(VSense, VP);
Random_Atrium = RA(ASense, AP);

Con_LRI = LRI(VSense, LRI_Start, VRP_Ex, LRI_Ex, VP, LRI_Stop, AVI_Done, AVI_Stop);

Con_VRP = VRP(VSense, VRP_Start, VRP_Ex, VP);
Timer_VRP = VRP_Timer(VRP_Ex, VRP_Start);

Timer_LRI = LRI_Timer(LRI_Start, LRI_Ex, LRI_Stop);
Monitor_LRI = LRI_Monitor(VSense, VP);
Monitor_VRP = VRP_Monitor(VSense, VP, VRP_Ex);

// Atrial Components
PVARP_i = PVARP(VP, VSense, AP, ASense, AEI_Done, PVARP_Done);
PVARP_Mon = PVARP_Monitor(ASense, AP, VSense, VP, PVARP_Done);

AVI_i = AVI(ASense, AVI_Done, AP, AVI_Stop);
AVI_Monitor_i = AVI_Monitor(ASense, AVI_Done, VP, AP);
AEI_i = AEI(ASense, VSense, VP, AP_Ex, AEI_Done, PVARP_Done, AP);
AEI_Mon = AEI_Monitor(VP, VSense, AP, AEI_Done);

// System Declaration.
system Random_Ventricle, Random_Atrium, Con_LRI, Con_VRP, Timer_LRI, Timer_VRP,
PVARP_i, AEI_i, AVI_i,
PVARP_Mon, AEI_Mon,
Monitor_VRP, Monitor_LRI;</system>
